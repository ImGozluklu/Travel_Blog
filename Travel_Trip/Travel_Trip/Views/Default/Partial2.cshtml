@using Travel_Trip.Models.Siniflar
@model List<Blog>
    
<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    <ul class="list list-pad">
        @{ int b = 0;}

        @foreach (var x in Model)
        {

            

            <li class="list-bg"><span>@{b++; } @b.</span><a href="/Blog/BlogDetay/@x.ID">@x.Baslik</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">Tüm Blogları Gör</a>
</div>